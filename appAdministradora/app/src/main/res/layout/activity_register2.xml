<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="chulo.paisa.com.appadministradora.controller.Register2Activity">
    <TextView
        android:id="@+id/txtDatosPer"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="DATOS PERSONALES"
        android:textColor="#000"
        android:textAlignment="center"
        android:textSize="35sp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintHorizontal_bias="0.506"
        app:layout_constraintVertical_bias="0.017" />


    <EditText
        android:id="@+id/textNombre"
        android:layout_width="350dp"
        android:layout_height="40dp"
        android:ems="10"
        android:hint="Digite nombre"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/txtDatosPer"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintHorizontal_bias="0.545"
        app:layout_constraintVertical_bias="0.148" />

    <EditText
        android:id="@+id/textApellido"
        android:layout_width="350dp"
        android:layout_height="40dp"
        android:ems="10"
        android:hint="Digite apellido"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/textNombre"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp"
        app:layout_constraintVertical_bias="0.0" />

    <EditText
        android:id="@+id/textCelular"
        android:layout_width="350dp"
        android:layout_height="40dp"
        android:ems="10"
        android:hint="Digite celular"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/textDocumento"
        app:layout_constraintVertical_bias="0.01" />

    <EditText
        android:id="@+id/textDocumento"
        android:layout_width="350dp"
        android:layout_height="40dp"
        android:ems="10"
        android:hint="Digite Documento"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/textApellido"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        tools:layout_editor_absoluteY="229dp" />

    <Button
        android:id="@+id/btnRegiDatos"
        android:layout_width="320sp"
        android:layout_height="70sp"
        android:text="Registtrar Datos"
        android:textSize="20sp"
        android:textColor="#fff"
        android:background="#000"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/textCelular"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintVertical_bias="0.211" />
</android.support.constraint.ConstraintLayout>